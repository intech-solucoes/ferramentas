export default class @(Model.ConfigEntidade.Nome)Entidade {
    @for (int i = 0; i < @Model.ColunasEntidade.Count; i++) {
        @Raw("\t\tpublic " + @Model.ColunasEntidade[i].Nome)

        if(@Model.ColunasEntidade[i].AceitaNulo && @Model.ColunasEntidade[i].Tipo != "string")
        {
            @Raw("?")
        }

        @Raw(": " + @Model.ColunasEntidade[i].Tipo + ";\n")
    }
}